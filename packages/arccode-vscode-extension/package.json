{
  "name": "arccode",
  "publisher": "arccode",
  "displayName": "Arccode",
  "description": "A game for developers",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "game",
    "rpg",
    "mmorpg"
  ],
  "activationEvents": [
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:go",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:php",
    "onLanguage:python",
    "onLanguage:ruby",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "arccode.signIn",
        "title": "Sign in",
        "category": "Arccode",
        "icon": "$(sign-in)"
      },
      {
        "command": "arccode.print",
        "title": "Print progress",
        "category": "Arccode",
        "icon": "$(list-flat)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "arccode.signIn",
          "group": "navigation@10"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "tsc && eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "22.x",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.92.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "array-differences": "^1.3.1",
    "language-keywords": "^0.1.1",
    "node-fetch": "^3.3.2",
    "tmp-promise": "^3.0.3",
    "uuid": "^10.0.0"
  }
}
